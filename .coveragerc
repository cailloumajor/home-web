# -*- mode: conf -*-
[run]
branch = True
omit =
    */tests/*
    */migrations/*
    gunicorn_config.py
    manage.py
    */wsgi.py
    core/management/commands/waitdb.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self\.debug
    if settings\.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
